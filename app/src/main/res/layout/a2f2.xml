<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".A2F2">

    <TextView
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/button22A"
        app:layout_constraintVertical_chainStyle="packed"
        android:id="@+id/hello22"
        android:layout_width="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_height="wrap_content"
        android:text="" />

    <Button
        android:id="@+id/button22A"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="57dp"
        android:text="@string/replace"
        app:layout_constraintBottom_toTopOf="@id/button22B"
        app:layout_constraintTop_toBottomOf="@id/hello22"
        app:layout_constraintVertical_chainStyle="packed" />

    <Button
        android:id="@+id/button22B"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="139dp"
        android:text="@string/remove"
        app:layout_constraintBottom_toTopOf="@id/button22C"
        app:layout_constraintTop_toBottomOf="@id/button22A"
        app:layout_constraintVertical_chainStyle="packed" />

    <Button
        android:id="@+id/button22C"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="210dp"
        android:text="@string/close"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/button22B"
        app:layout_constraintVertical_chainStyle="packed" />

</RelativeLayout>